{
    "name": "devsfort/laravel-whatsapp-chat",
    "description": "A comprehensive WhatsApp Business Cloud API integration package for Laravel with real-time chat functionality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Haseeb Ahmad",
            "email": "haseeb@devsfort.com"
        }
    ],
    "keywords": [
        "laravel",
        "whatsapp",
        "chat",
        "messaging",
        "real-time",
        "pusher",
        "broadcasting",
        "api",
        "business",
        "cloud",
        "vue",
        "inertia"
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "*",
        "illuminate/database": "*",
        "illuminate/http": "*",
        "illuminate/broadcasting": "*",
        "illuminate/console": "*",
        "illuminate/events": "*",
        "illuminate/queue": "*",
        "pusher/pusher-php-server": "*",
        "guzzlehttp/guzzle": "*",
        "inertiajs/inertia-laravel": "*",
        "tightenco/ziggy": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "DevsFort\\LaravelWhatsappChat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevsFort\\LaravelWhatsappChat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevsFort\\LaravelWhatsappChat\\WhatsAppChatServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
